function slot_synonyms(slot)
    "{IMPOSSIBLEREGEX()}"

    if slot = "name"
        "name"
        "{domain_vocab()}('s)? name"
        "name of the {domain_vocab()}"
        "title"
        "designation"
    if slot = "price"
        "cost(s)?"
        "prices"
        "pricing"
        "fee(s)?"
        "expensive"
        "cheap"
    if slot = "rating":
        "score"
        "evaluation"
        "ranking"
        "stars"
    if slot = "bar"
        "bar(s)?"
        "*bar(s)?"
        "restaurant(s)?"
        "pub(s)?"
    if slot = "location"
        "place"
        "site"
        "position"
        "where"
    if slot = "phone"
        "(telephone|phone|contact)((\s|\b)number)?"
        "number"
    if slot = "hours"
        "(business|operating|opening)+hours"
        "schedule"
        "open(s)?"
        "closed"
        "availabel"
        "time"
        "(a|p)m"
    if slot = "website"
        "(official )?site"
        "(web )?page"
        "web"
        "online( presence)?"
        "internet"
    if slot = "review"
        "(customer|user|guest('s|s')?)?((feedback|review|opinion|comment|experience|evaluation|critique)(s)?)"
    if slot = "planning"
        "reserve"
        "book"
        "schedule"
        "secure"
        "arrange"
        "confirm"
        "table"
    if slot = "payment"
        "pay"
        "(mobile )?payment(s)?"
        "transaction(s)?"
        "billing"
        "fee(s)?"
        "cost(s)?"
        "purchase(s)?"
        "cash"
        "(credit|debit )?card(s)?"
        "paypal"
    if slot = "highlight"
        "highlights"
        "standout(s)?"
        "key point(s)?"
        "important aspect(s)?"
        "(main )?attraction(s)?"
        "significant element(s)?"
        "special characteristic(s)?"
